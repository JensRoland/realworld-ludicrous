{
    "name": "realworld/ludicrous",
    "description": "RealWorld App with Vanilla PHP and a sprinkle of JS",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\Lib\\": "lib/",
            "App\\Models\\": "models/",
            "App\\Services\\": "services/",
            "App\\Migrations\\": "../database/migrations/"
        },
        "files": [
            "components/article-actions/controller.php",
            "components/article-list/controller.php",
            "components/article-meta/controller.php",
            "components/article-preview/controller.php",
            "components/comment/controller.php",
            "components/comment-form/controller.php",
            "components/comment-list/controller.php",
            "components/error-messages/controller.php",
            "components/favorite-button/controller.php",
            "components/feed-toggle/controller.php",
            "components/follow-button/controller.php",
            "components/navbar/controller.php",
            "components/pagination/controller.php",
            "components/profile-tabs/controller.php",
            "components/tag-input/controller.php",
            "components/tag-list/controller.php",
            "components/user-info/controller.php"
        ]
    },
