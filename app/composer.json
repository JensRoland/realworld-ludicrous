        "erusev/parsedown": "^1.7",
        "firebase/php-jwt": "^6.10",
        "symfony/yaml": "^6.0 || ^7.0",
        "doctrine/dbal": "^4.3",
        "latte/latte": "^3.1",
        "doctrine/migrations": "^3.8",
        "symfony/console": "^6.0 || ^7.0",
        "symfony/stopwatch": "^6.0 || ^7.0",
        "symfony/var-exporter": "^6.2 || ^7.0",
        "symfony/string": "^6.0 || ^7.0",
        "nette/utils": "^4.1"
